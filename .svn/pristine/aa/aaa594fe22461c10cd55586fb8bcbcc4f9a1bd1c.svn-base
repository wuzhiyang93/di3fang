import request from '@/utils/request';

/**
 *分页查询小程序专题
 * @param query 查询参数
 */
export function queryAppletThematic(query) {
  return request({
    url: 'appletthematic',
    method: 'get',
    params: query
  })
}

/**
 * 删除小程序专题
 * @param id 专题id
 */
export function deleteAppletThematic(id) {
  return request({
    url: 'appletthematic/' + id,
    method: 'delete',
  })
}
