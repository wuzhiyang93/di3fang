import request from '@/utils/request';


/**
 * 分页查询新增会员统计（按日期分组）
 * @param query 查询参数
 */
export function queryNewCustomerStatisticsWithPage(query) {
  return request({
    url: 'newmembers/statistics',
    method: 'get',
    params: query
  })
}



/**
 * 查询新增会员统计（按日期分组）
 * @param query 查询参数
 */
export function queryNewCustomerStatistics(query) {
  return request({
    url: 'newmembers',
    method: 'get',
    params: query
  })
}
