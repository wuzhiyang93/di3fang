import request from '@/utils/request';


/**
 * 根据id查询会员详情
 *
 * @param customerId 会员id
 */
export function queryCustomerDetailById(customerId) {
    return request({
        url: 'customer/detail/' + customerId,
        method: 'get',
    })
}
