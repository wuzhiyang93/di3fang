/**
 * 短信设置路由
 *
 * @author Caizize created on 2019/5/27
 */

import request from '@/utils/request';


/**
 * 查询短信设置
 */
export function querySmsSet() {
  return request({
    url: 'smsset',
    method: 'get',
  })
}

/**
 * 修改短信设置
 *
 * @param smsSet 短信设置信息
 */
export function updateSmsSet(smsSet) {
  return request({
    url: 'smsset',
    method: 'put',
    data: smsSet
  })
}
