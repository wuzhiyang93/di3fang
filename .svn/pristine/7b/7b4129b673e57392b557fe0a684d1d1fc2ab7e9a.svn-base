import request from '@/utils/request';


/**
 * 分页查询新增店铺统计
 * @param query 查询参数
 */
export function queryNewStoreInfoStatisticsWithPage(query) {
  return request({
    url: 'newstore/statistics',
    method: 'get',
    params: query
  })
}



/**
 * 查询新增店铺统计
 * @param query 查询参数
 */
export function queryNewStoreInfoStatistics(query) {
  return request({
    url: 'newstore',
    method: 'get',
    params: query
  })
}
