import request from '@/utils/request';

/**
 *分页查询app专题
 * @param query 查询参数
 */
export function queryAppThematic(query) {
  return request({
    url: 'appthematic',
    method: 'get',
    params: query
  })
}

/**
 * 删除app专题
 * @param id 专题id
 */
export function deleteAppThematic(id) {
  return request({
    url: 'appthematic/' + id,
    method: 'delete',
  })
}
